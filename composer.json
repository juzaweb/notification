{
    "name": "juzaweb/notification",
    "type": "juzaweb-plugin",
    "description": "Notification plugin for Juzaweb cms",
    "keywords": [
        "juzaweb",
        "notification",
        "cms",
        "plugin"
    ],
    "license": "MIT",
    "homepage": "https://juzaweb.com",
    "require": {
        "php": "^8.0|^8.1"
    },
    "require-dev": {
        "laravel/laravel": "^6.0 | ^7.0 | ^8.0",
        "phpunit/phpunit": "6.0 | 7.0 | 7.5 | 8.4 | ^8.5",
        "juzaweb/modules": "^1.0",
        "juzaweb/dev-tool": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Juzaweb\\Notification\\": "src/"
        }
    },
    "extra": {
        "juzaweb": {
            "name": "Notification",
            "domain": "juno",
            "providers": [
                "Juzaweb\\Notification\\Providers\\NotificationServiceProvider"
            ]
        }
    }
}
