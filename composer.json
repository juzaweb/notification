{
    "name": "juzaweb/notification",
    "type": "juzaweb-plugin",
    "description": "Notification plugin for Juzaweb cms",
    "keywords": [
        "juzaweb",
        "notification",
        "cms",
        "plugin"
    ],
    "license": "MIT",
    "homepage": "https://juzaweb.com",
    "require": {
        "php": "^8.0|^8.1"
    },
    "require-dev": {
        "juzaweb/modules": "^1.0",
        "juzaweb/dev-tool": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Juzaweb\\Notification\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Juzaweb\\Notification\\Tests\\": "tests/"
        }
    },
    "extra": {
        "juzaweb": {
            "name": "Notification",
            "domain": "jw_notification",
            "networkable": true,
            "providers": [
                "Juzaweb\\Notification\\Providers\\NotificationServiceProvider"
            ]
        }
    }
}
